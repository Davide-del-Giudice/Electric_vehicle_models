#/usr/bin/csh

pan k1.pan -ad INCLUDE_EV -ad SAVE -ad FULL -ad CPCV -ad LFP -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad FULL -ad CPCV -ad LMO -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad FULL -ad CPCV -ad NMC -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad FULL -ad CPCV -ad NCA -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad FULL -ad CCCV -ad LFP -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad FULL -ad CCCV -ad LMO -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad FULL -ad CCCV -ad NMC -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad FULL -ad CCCV -ad NCA -q -w run1&

pan k1.pan -ad INCLUDE_EV -ad SAVE -ad SYNTHETIC -ad CPCV -ad LFP -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad SYNTHETIC -ad CPCV -ad LMO -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad SYNTHETIC -ad CPCV -ad NMC -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad SYNTHETIC -ad CPCV -ad NCA -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad SYNTHETIC -ad CCCV -ad LFP -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad SYNTHETIC -ad CCCV -ad LMO -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad SYNTHETIC -ad CCCV -ad NMC -q -w run1&
pan k1.pan -ad INCLUDE_EV -ad SAVE -ad SYNTHETIC -ad CCCV -ad NCA -q -w run1&

pan k1.pan -ad INCLUDE_EV -ad SAVE -ad PQ -q -w run1&


