#/usr/bin/csh
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad CPCV -ad LFP -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad CPCV -ad LMO -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad CPCV -ad NMC -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad CPCV -ad NCA -q -w run1&

pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad CCCV -ad LFP -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad CCCV -ad LMO -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad CCCV -ad NMC -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad CCCV -ad NCA -q -w run1&


pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad SYNTHETIC -ad CPCV -ad LFP -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad SYNTHETIC -ad CPCV -ad LMO -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad SYNTHETIC -ad CPCV -ad NMC -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad SYNTHETIC -ad CPCV -ad NCA -q -w run1&

pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad SYNTHETIC -ad CCCV -ad LFP -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad SYNTHETIC -ad CCCV -ad LMO -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad SYNTHETIC -ad CCCV -ad NMC -q -w run1&
pan static_EV_sweep.pan -ad SWEEP -ad SAVE -ad SYNTHETIC -ad CCCV -ad NCA -q -w run1&

